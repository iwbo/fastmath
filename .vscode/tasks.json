{
    "version": "0.1.0",
    "command": "cmd",
    "args": ["/C"],
    "isShellCommand": false,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [
        {
            "taskName": "npm install",
            "args": ["npm install"]
        },
        {
            "taskName": "webpack build",
            "args": ["webpack"]
        },
        {
            "taskName": "typescript build",
            "args": ["tsc server"]
        }
    ]
}